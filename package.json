{
  "name": "spiritislandweb",
  "version": "0.0.1",
  "private": true,
  "description": "multiplayer implementation of boardgame spirit island for the browser.",
  "main": "index.js",
  "repository": "https://github.com/maddinpsy/spiritislandweb.git",
  "author": "Martin Hoffmann <maddinpsycho@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "bgio-postgres": "^1.0.13",
    "boardgame.io": "^0.44.0",
    "classnames": "^2.2.6",
    "esm": "^3.2.25",
    "i18next": "^20.2.1",
    "i18next-browser-languagedetector": "^6.0.1",
    "koa-static": "^5.0.0",
    "panzoom": "^9.4.2",
    "point-in-polygon": "^1.1.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-i18next": "^11.8.6",
    "react-router-dom": "^5.2.0",
    "react-scripts": "4.0.3",
    "react-transition-group": "^4.4.1"
  },
  "scripts": {
    "build": "react-scripts build && tsc --project tsconfig-server.json",
    "start": "node -r esm build/server.js",
    "start-dev": "yarn react-scripts start & yarn serve-watch",
    "serve-watch": "tsc-watch --project tsconfig-server.json --watch --onSuccess 'node -r esm build/server.js'",
    "testui": "react-scripts test",
    "eject": "react-scripts eject",
    "test": "jest"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.14.0",
    "@babel/preset-env": "^7.14.1",
    "@babel/preset-typescript": "^7.13.0",
    "@types/classnames": "^2.2.11",
    "@types/jest": "^26.0.23",
    "@types/koa-static": "^4.0.1",
    "@types/node": "^14.14.37",
    "@types/point-in-polygon": "^1.0.0",
    "@types/react": "^17.0.3",
    "@types/react-dom": "^17.0.3",
    "@types/react-router-dom": "^5.1.7",
    "@types/react-transition-group": "^4.4.1",
    "babel-jest": "^26.6.3",
    "babel-plugin-module-resolver": "^4.1.0",
    "inotify": "https://github.com/c4milo/node-inotify.git",
    "node-fetch": "^2.6.1",
    "jest": "^26.6.3",
    "node-sass": "^5.0.0",
    "tsc-watch": "^4.2.9",
    "typescript": "^4.1.3"
  },
  "engines": {
    "node": "14.x"
  }
}
